{
  "name": "cui-a2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "internal:ng": "ng",
    "internal:start": "ng serve",
    "internal:build": "ng build --prod",
    "internal:test": "ng test -sr",
    "internal:test:watch": "ng test",
    "internal:lint": "ng lint",
    "internal:e2e": "ng e2e",
    "internal:stubby": "stubby -d stubby/config.json",
    "internal:serve": "ng serve --host 0.0.0.0",
    "internal:serve:stubby": "concurrently --kill-others \"npm run internal:stubby\" \"npm run internal:serve\" ",
    "docker:test": "docker run --rm -t -v $(pwd):/opt/app $npm_package_config_dockerImage npm run internal:test",
    "docker:test:watch": "docker run --rm -t -v $(pwd):/opt/app $npm_package_config_dockerImage npm run internal:test:watch",
    "docker:serve": "docker run --rm -t -p 4200:4200  -v $(pwd):/opt/app $npm_package_config_dockerImage npm run internal:serve",
    "docker:serve:stubby": "docker run --rm -t -p 4200:4200 -p 8882:8882 -v $(pwd):/opt/app $npm_package_config_dockerImage npm run internal:serve:stubby"
  },
  "config": {
    "dockerImage": "node-angular-cli:9.3.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.0",
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@angular/router": "5.0.0",
    "core-js": "2.4.1",
    "rxjs": "5.5.2",
    "stacktrace-js": "2.0.0",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "5.0.0",
    "@angular/language-service": "5.0.0",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/node": "6.0.60",
    "@types/stacktrace-js": "0.0.32",
    "codelyzer": "4.0.1",
    "concurrently": "3.5.1",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-spec-reporter": "0.0.32",
    "phantomjs-prebuilt": "2.1.16",
    "protractor": "5.1.2",
    "stubby": "4.0.0",
    "ts-mockito": "2.2.7",
    "ts-node": "3.2.0",
    "tslint": "5.7.0",
    "typescript": "2.4.2"
  }
}
